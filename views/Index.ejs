<!DOCTYPE html>
<html lang="en" ng-app="cadmium">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no">

    <title>Cadmium</title>

    <script src="../public/javascripts/jquery-2.1.4.min.js"></script>
    <script src="../node_modules/angular/angular.min.js"></script>
    <script src="../public/javascripts/Angular/angular-route.min.js"></script>
    <script src="../public/javascripts/Angular/ui-bootstrap-tpls-0.14.2.min.js"></script>
    <script src="../public/javascripts/socket.io.js"></script>
    <script src="../public/javascripts/bootstrap.min.js"></script>
    <script src="../public/javascripts/html2canvas.js" type="text/javascript"></script>
    <script src="../public/javascripts/dynamicTable.js" type="text/javascript"></script>

    <style>
        @import url(../public/stylesheets/bootstrap.min.css);
        @import url(../public/stylesheets/cubism.css);
        @import url(../public/javascripts/Cesium/Widgets/widgets.css);
        @import url(../public/javascripts/milstd2525/renderer.css);
        @import url(../public/stylesheets/style.css);
        @import url(../public/stylesheets/font.css);
    </style>
</head>
<body id="body" ui-view="body">

<!--REQUIRED-->
<script src="../public/javascripts/d3/d3.min.js" type="text/javascript"></script>
<script src="../public/javascripts/d3/hexbin.js" type="text/javascript"></script>
<script src="../public/javascripts/cubism/cubism.v1.min.js" type="text/javascript"></script>

<!--CESIUM INIT-->
<div id="cesiumContainer"></div>
<script src="../public/javascripts/milstd2525/sm-bc.min.js" type="text/javascript"></script>
<script src="../public/javascripts/Cesium/Cesium.js" type="text/javascript"></script>
<script src="../public/javascripts/Cadmium.js" type="text/javascript"></script>

<!--DEPENDENCY INIT-->
<script src="../views/dependencies/Listeners.js"></script>
<script src="../views/dependencies/Cs.js"></script>
<script src="../views/dependencies/Common.js"></script>
<script src="../views/dependencies/EntitySelector.js"></script>
<script src="../views/dependencies/Create.js"></script>
<script src="../views/dependencies/Acquire.js"></script>
<script src="../views/dependencies/Vapor.js"></script>
<script src="../views/dependencies/AdvancedSim.js"></script>
<script src="../views/dependencies/BirdsEye.js"></script>
<div id="dependencies"></div>

<!--GLOBAL DATA MODEL INIT-->
<script src="../views/globalDataModel/Model.js"></script>
<script src="../views/globalDataModel/globalDataModel.js"></script>
<script src="../views/globalDataModel/modelTemplates.js"></script>

<!--ANGULAR INIT-->
<script src="../views/cadmium.module.js"></script>
<script src="../views/cadmium.factory.js"></script>
<script src="../views/UI/menu.directive.js"></script>
<script src="../views/UI/leftUtil.directive.js"></script>
<script src="../views/UI/rightUtil.directive.js"></script>
<script src="../views/UI/footer.directive.js"></script>
<script src="../views/UI/modal.directive.js"></script>


<nav>
    <div id="baOne" class="backAng"></div>
    <div id="menuCont">
        <img id="mainLogo" src="../public/images/cadmiumLogo.svg" />
    </div>
    <div id="menu" ng-controller="menuCtrl">
        <div menu data="data.getMenu" model="proto"></div>
    </div>
    <div id="trOne"></div>
    <div id="trTwo"></div>
</nav>
<div ng-controller="leftUtilCtrl">
    <div id="leftHide" style="left: -22.75em">
        <div id="tabContainer">
            <div id="tabs">
                <ul>
                    <li id="tabHeader_1" class="bl" ng-repeat="tab in data.getLeftUtil">{{tab.title}}</li>
                </ul>
            </div>
            <div id="tabscontent">
                <div leftutil ng-repeat="item in data.getLeftUtil" item="item" model="proto" toggle="toggle" class="tabpage" id="{{'tabpage_' + $index}}"></div>
            </div>
        </div>
        <div id="leftHideToggle" class="hideToggle">
            <a href="#" class="ht" id="lhToggleA" ng-click="proto.slideLeft()"><span class="glyphicon glyphicon-menu-right" aria-hidden="true"></span></a>
        </div>
    </div>
</div>
<div ng-controller="rightUtilCtrl">
    <div id="rightHide" style="right: -19.75em">
        <div id="rightHideToggle" class="hideToggle">
            <a href="javascript:;" class ="ht" id="rhToggleA" ng-click="proto.slideRight();"><span class="glyphicon glyphicon-menu-left" aria-hidden="true"></span></a>
        </div>
        <div id="rightBar">
            <div rightutil="rightutil" ng-repeat="item in data.getRightUtil"></div>
        </div>
    </div>
</div>
<div ng-controller="footerCtrl">
    <div id="footer"></div>
    <div id="bottom">
        <div footer="footer" ng-repeat="item in data.getFooter"></div>
    </div>
</div>
<div ng-controller="modalCtrl">
    <div modals data="data.getModals" model="proto"></div>
</div>

</body>
</html>
